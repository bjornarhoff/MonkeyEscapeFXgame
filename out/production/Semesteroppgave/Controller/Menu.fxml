            <Button mnemonicParsing="false" mouseTransparent="false" onMouseClicked="#loadGame" prefHeight="72.0" prefWidth="245.0" styleClass="MenuButton" text="LOAD GAME " textFill="WHITE" textOverrun="CLIP">
