<AnchorPane fx:id="rootPane" minHeight="650.0" minWidth="650.0" prefHeight="650.0" prefWidth="650.0" style="-fx-background-color: BLACK;" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MenuController">
   <children>
      <Label layoutX="96.0" layoutY="97.0" prefHeight="95.0" prefWidth="480.0" text="MonkeyEscape" textFill="#d06614">
         <font>
            <Font name="Herculanum" size="64.0" />
         </font>
      </Label>
      <ImageView fitHeight="108.0" fitWidth="187.0" layoutX="282.0" layoutY="237.0" pickOnBounds="true" preserveRatio="true" style="visibility: TRUE;">
         <image>
            <Image url="@../spillet/ape.png" />
         </image></ImageView>
      <VBox layoutX="216.0" layoutY="404.0" prefHeight="198.0" prefWidth="240.0">
